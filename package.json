{
  "devDependencies": {
    "@types/express": "^4.17.21",
    "esbuild": "0.20.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "start": "ts-node server.ts --isDev=true",
    "start-prod": "node server.js --isDev=false",
    "build": "tsc -p tsconfig.build.json",
    "bundle": "ts-node bundle.ts",
    "deploy-prod": "npm run bundle;cp -r output ../;cd ../output;npm i;npm run start-prod"
  },
  "dependencies": {
    "express": "^4.18.3",
    "path": "^0.12.7"
  }
}
