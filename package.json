{
  "devDependencies": {
    "@types/express": "^4.17.21",
    "esbuild": "0.20.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "start": "ts-node server.ts --isDev=true",
    "start-prod": "node server.js --isDev=false",
    "build": "tsc -p tsconfig.build.json",
    "bundle": "ts-node bundle.ts",
    "deploy-prod": "npm i --omit=dev;npm run bundle;mv node_modules output/node_modules;cd output;npm run start-prod"
  },
  "dependencies": {
    "express": "^4.18.3"
  }
}
